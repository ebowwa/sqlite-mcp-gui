{
  "name": "sqlite-mcp-gui",
  "version": "1.0.0",
  "description": "SQLite MCP GUI - A web interface for SQLite databases using MCP protocol",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/ui/server.js",
    "start:mcp": "node dist/server/index.js",
    "start:ui": "node dist/ui/server.js",
    "dev": "tsx watch src/ui/server.ts",
    "cli": "tsx src/cli/index.ts",
    "mcp": "tsx src/server/index.ts",
    "create-example-db": "tsx src/scripts/create-example-db.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "sqlite",
    "mcp",
    "gui",
    "database",
    "web-ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
